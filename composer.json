{
    "name": "danielgp/bank-holidays",
    "description": "Usefull functions to return list of all bank holidays per year and month plus working days in month",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Popiniuc",
            "email": "danielpopiniuc@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.2"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "~0.4",
        "phpunit/phpunit": "^7.2",
        "phpunit/php-code-coverage": "^6",
        "squizlabs/php_codesniffer": "^3.3",
        "wimg/php-compatibility": "~8.1"
    },
    "autoload": {
        "classmap": [
            "source/Romanian.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "source/Romanian.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "archive": {
        "exclude": ["/nbproject", "/vendor", "/tests"]
    }
}
